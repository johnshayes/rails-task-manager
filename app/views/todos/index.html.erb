<h1> All the To Do items</h1>


<%= link_to "Create new", new_todo_path %>


<% @todos.each do |todo| %>
  <h2><%= todo.title%></h2>
  <%= link_to "Task details", todo_path(todo) %>
  <%= link_to "Edit task", edit_todo_path(todo) %>
  <%= link_to "Delete task", todo_path(todo), method: :delete %>
<% end %>






